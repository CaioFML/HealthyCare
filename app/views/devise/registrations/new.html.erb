                        <h2>Novo cadastro</h2>
                        <p class="notice"><%= notice %></p>
                        <p class="alert"><%= alert %></p>
                        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                            <p>
                                <div class="form">
                                    <label for="login">e-mail</label>
                                    <input type="email" id="login" name=user[email] class="form-control">
                                </div>
                            </p>
                            <p>
                                <div class="form">
                                    <label for="senha">senha</label>
                                    <input type="password" id="senha" name="user[password]" class="form-control">
                                </div>
                            </p>

                            <p>
                                <div class="form">
                                    <label for="senha">senha</label>
                                    <input type="password" id="password_confirmation" name="user[password_confirmation]" class="form-control">
                                </div>
                            </p>
                        
                            <div class="row">
                                <div class="col-lg-4 col-sm-12">
                                    <input type="submit" class="btn btn-primary btn-action btn-fill" value="Cadastrar">
                                </div>
                                
                            </div>
                            
                        <%end%>
                    </div>
                </div>
            </div>
        </div>
