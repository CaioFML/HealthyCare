      <div class="flex-features" id="login">
        <div class="container">
            <div class="flex-split">
                <div class="f-left wow fadeInUp" data-wow-delay="0s">
                    <div class="left-content">
                        <img class="img-fluid" src="/landy/assets/images/feature_bg.jpg" alt="">
